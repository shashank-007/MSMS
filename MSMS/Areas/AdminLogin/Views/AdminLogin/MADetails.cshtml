
@{
    ViewBag.Title = "MADetails";
    Layout = "~/Areas/AdminLogin/Views/Shared/_Layout.cshtml";
}



<script src="~/Scripts/jquery-1.12.4.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        if (window.sessionStorage.getItem("AdminID") == null) {
            window.location = "Login";
        }
        else {
            var AdminType = $("#txtAdminType");
            var AdminId = $("#txtAdminId");
            var Name = $("#txtName");
            var Age = $("#txtAge");
            var Gender = $("#txtGender");
            var Phone = $("#txtPhone");
            var PanNum = $("#txtPan");
            var Aadharno = $("#txtAadhar");
            var PAddress = $("#txtPAddress");
            var CAddress = $("#txtCAddress");
            var Photo = $("#txtPhoto");
            var Status = $("#txtStatus");
            var Ref = $("#txtRef");

            $.ajax({
                type: "get",
                url: "https://localhost:44339/api/AdminLogin/WebApiMADDashBoard/GetAdminByID/"+ window.sessionStorage.getItem("AdminID"),
                dataType: "json",
                success: function (data) {
                    alert(data.Admin_Type);
                    AdminId.text(data.Admin_ID);
                    Name.text(data.Name);
                    Age.text(data.Age);
                    Gender.text(data.Gender);
                    Phone.text(data.Phone);
                    PanNum.text(data.Pan_Number);
                    Aadharno.text(data.Aadhar_Number);
                    PAddress.text(data.Permanent_Address);
                    CAddress.text(data.Current_Address);
                    Photo.attr("src", "data:image/jpg;base64," + data.Photo);
                    Status.text(data.Status);
                    Ref.text(data.Reference);
                },
                error: function (XHR) {
                    alert('error');
                }
            });
        }

    });
</script>


